############################## COMPILE VAR #####################################

RC			=	rustc
NAME		=	Colleen

############################## SOURCES #########################################

SRC			=	Colleen.rs

############################## BUILD ###########################################

all: $(NAME)

$(NAME): $(SRC)
	$(RC) $<

############################## CLEANUP #########################################

clean:

fclean: clean
	rm -f $(NAME)

re: fclean all

.PHONY: all clean fclean re
